{
  "name": "jdckl-nextjs-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "postbuild": "next-sitemap",
    "prepare": "husky install",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy --preview-feature && yarn db:generate",
    "db:gen:migration": "prisma migrate dev --preview-feature",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "3.12.0",
    "argon2": "^0.28.5",
    "axios": "^0.26.1",
    "clsx": "^1.1.1",
    "cookie": "^0.5.0",
    "framer-motion": "^6.2.8",
    "jsonwebtoken": "^8.5.1",
    "next": "^12.1.0",
    "next-connect": "^0.12.2",
    "prisma": "^3.12.0",
    "rate-limiter-flexible": "^2.3.6",
    "react": "^17.0.2",
    "react-datepicker": "^4.7.0",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.35.0",
    "react-select": "^5.3.2",
    "react-table": "^7.7.0",
    "superjson": "^1.8.1",
    "tailwind-merge": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@svgr/webpack": "^6.2.1",
    "@tailwindcss/forms": "^0.5.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/cookie": "^0.5.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pdfkit": "^0.12.6",
    "@types/react": "^17.0.40",
    "@types/react-datepicker": "^4.4.1",
    "@types/react-table": "^7.7.11",
    "@types/supertest": "^2.0.12",
    "@types/tailwindcss": "^2.2.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.5",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^11.2.6",
    "msw": "^0.42.0",
    "next-router-mock": "^0.6.10",
    "next-sitemap": "^1.9.12",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "standard-version": "^9.3.2",
    "supertest": "^6.2.3",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "resolutions": {
    "autoprefixer": "10.4.5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  }
}
